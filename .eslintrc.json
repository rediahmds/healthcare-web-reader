{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "comma-dangle": ["error", "only-multiline"]
    }
}
